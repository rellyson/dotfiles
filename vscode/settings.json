{
  "workbench.colorTheme": "Roxo theme",
  "workbench.iconTheme": "vscode-icons",
  "workbench.productIconTheme": "icons-carbon",
  "workbench.tips.enabled": false,
  "workbench.activityBar.location": "top",
  "workbench.colorCustomizations": {
    "statusBar.background": "#120d17",
    "statusBar.noFolderBackground": "#120d17",
    "statusBar.debuggingBackground": "#120d17",
    "statusBar.foreground": "#edecee",
    "statusBar.debuggingForeground": "#edecee"
  },
  "workbench.startupEditor": "none",
  "security.workspace.trust.untrustedFiles": "open",
  "window.customTitleBarVisibility": "auto",
  "window.menuBarVisibility": "classic",
  "window.titleBarStyle": "custom",
  "window.zoomLevel": 1.3,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "editor.fontFamily": "'FiraCode Nerd Font Mono', 'monospace'",
  "editor.fontLigatures": true,
  "editor.autoClosingBrackets": "always",
  "editor.codeLens": false,
  "editor.formatOnSave": true,
  "editor.fontSize": 13,
  "editor.lineNumbers": "relative",
  "editor.minimap.enabled": false,
  "editor.inlineSuggest.suppressSuggestions": true,
  "explorer.expandSingleFolderWorkspaces": false,
  "explorer.compactFolders": false,
  "git.openRepositoryInParentFolders": "never",
  "files.exclude": {
    "**/__pycache__": true
  },
  "files.associations": {
    "nuget.config": "xml",
    "alert.rules": "yaml",
  },
  "terminal.external.linuxExec": "/bin/zsh",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.fontSize": 13,
  "telemetry.telemetryLevel": "off",
  "vim.cursorStylePerMode.insert": "line",
  "vim.cursorStylePerMode.normal": "block",
  "vim.enableNeovim": true,
  "vim.shell": "/bin/zsh",
  "vim.leader": "<Space>",
  "vim.hlsearch": true,
  "vim.easymotion": true,
  "vim.useSystemClipboard": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<S-h>"
      ],
      "commands": [
        ":bprevious"
      ]
    },
    {
      "before": [
        "<S-l>"
      ],
      "commands": [
        ":bnext"
      ]
    },
    {
      "before": [
        "leader",
        ","
      ],
      "commands": [
        ":tabprevious"
      ]
    },
    {
      "before": [
        "leader",
        "."
      ],
      "commands": [
        ":tabnext"
      ]
    },
    {
      "before": [
        "leader",
        "v"
      ],
      "commands": [
        ":vsplit"
      ]
    },
    {
      "before": [
        "leader",
        "s"
      ],
      "commands": [
        ":split"
      ]
    },
    {
      "before": [
        "leader",
        "h"
      ],
      "commands": [
        "workbench.action.focusLeftGroup"
      ]
    },
    {
      "before": [
        "leader",
        "Left"
      ],
      "commands": [
        "workbench.action.focusLeftGroup"
      ]
    },
    {
      "before": [
        "leader",
        "j"
      ],
      "commands": [
        "workbench.action.focusBelowGroup"
      ]
    },
    {
      "before": [
        "leader",
        "k"
      ],
      "commands": [
        "workbench.action.focusAboveGroup"
      ]
    },
    {
      "before": [
        "leader",
        "l"
      ],
      "commands": [
        "workbench.action.focusRightGroup"
      ]
    },
    {
      "before": [
        "leader",
        "Right"
      ],
      "commands": [
        "workbench.action.focusRightGroup"
      ]
    },
    {
      "before": [
        "leader",
        "w"
      ],
      "commands": [
        ":w!"
      ]
    },
    {
      "before": [
        "leader",
        "q"
      ],
      "commands": [
        ":q!"
      ]
    },
    {
      "before": [
        "leader",
        "x"
      ],
      "commands": [
        ":x!"
      ]
    },
    {
      "before": [
        "leader",
        "f"
      ],
      "commands": [
        "workbench.action.quickOpen"
      ]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": [
        "<"
      ],
      "commands": [
        "editor.action.outdentLines"
      ]
    },
    {
      "before": [
        ">"
      ],
      "commands": [
        "editor.action.indentLines"
      ]
    },
    {
      "before": [
        "J"
      ],
      "commands": [
        "editor.action.moveLinesDownAction"
      ]
    },
    {
      "before": [
        "K"
      ],
      "commands": [
        "editor.action.moveLinesUpAction"
      ]
    },
    {
      "before": [
        "leader",
        "c"
      ],
      "commands": [
        "editor.action.commentLine"
      ]
    }
  ],
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.normal": [
    "#120d17",
    "#edecee"
  ],
  "vim.statusBarColors.insert": [
    "#8ED9CD",
    "#120d17"
  ],
  "vim.statusBarColors.replace": [
    "#F38BA8",
    "#120d17"
  ],
  "vim.statusBarColors.commandlineinprogress": [
    "#FAB387",
    "#120d17"
  ],
  "vim.statusBarColors.searchinprogressmode": [
    "#FAB387",
    "#120d17"
  ],
  "vim.statusBarColors.visualblock": [
    "#CBA6F7",
    "#120d17"
  ],
  "vim.statusBarColors.visualline": [
    "#CBA6F7",
    "#120d17"
  ],
