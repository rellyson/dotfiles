{
  "workbench.colorTheme": "Roxo theme",
  "workbench.iconTheme": "vscode-icons",
  "workbench.productIconTheme": "Tabler",
  "workbench.tips.enabled": false,
  "workbench.colorCustomizations": {
    "statusBar.foreground": "#edecee",
    "statusBar.noFolderBackground": "#120d17",
    "statusBar.debuggingBackground": "#120d17",
    "statusBar.debuggingForeground": "#edecee",
    "statusBar.background": "#120d17"
  },
  "window.zoomLevel": 2,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "editor.fontFamily": "'Hack Nerd Font', 'monospace'",
  "editor.cursorStyle": "block",
  "editor.autoClosingBrackets": "always",
  "editor.fontLigatures": true,
  "editor.codeLens": false,
  "editor.formatOnSave": true,
  "editor.fontSize": 15,
  "editor.minimap.enabled": false,
  "explorer.expandSingleFolderWorkspaces": false,
  "explorer.compactFolders": false,
  "git.openRepositoryInParentFolders": "never",
  "files.exclude": {
    "**/__pycache__": true
  },
  "terminal.external.linuxExec": "/bin/zsh",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "vsicons.dontShowNewVersionMessage": true,
  "window.titleBarStyle": "custom",
  "yaml.validate": true,
  "yaml.completion": true,
  "redhat.telemetry.enabled": false,
  "code-runner.clearPreviousOutput": true,
  "hediet.vscode-drawio.theme": "dark",
  "go.toolsManagement.autoUpdate": true,
  "vim.cursorStylePerMode.insert": "line",
  "vim.cursorStylePerMode.normal": "block",
  "vim.enableNeovim": true,
  "vim.shell": "/bin/zsh",
  "vim.leader": " ",
  "vim.smartRelativeLine": true,
  "vim.useSystemClipboard": true,
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.normal": [
    "#120d17",
    "#edecee"
  ],
  "vim.statusBarColors.insert": [
    "#8ED9CD",
    "#120d17"
  ],
  "vim.statusBarColors.replace": [
    "#F38BA8",
    "#120d17"
  ],
  "vim.statusBarColors.commandlineinprogress": [
    "#FAB387",
    "#120d17"
  ],
  "vim.statusBarColors.searchinprogressmode": [
    "#FAB387",
    "#120d17"
  ],
  "vim.statusBarColors.visualblock": [
    "#CBA6F7",
    "#120d17"
  ],
  "vim.statusBarColors.visualline": [
    "#CBA6F7",
    "#120d17"
  ],
  "vsicons.associations.folders": [
    {
      "icon": "server",
      "extensions": [
        "httpserver",
        "http"
      ]
    },
    {
      "icon": "git",
      "extensions": [
        ".githooks"
      ]
    },
    {
      "icon": "private",
      "extensions": [
        "internal"
      ]
    },
    {
      "icon": "minikube",
      "extensions": [
        "charts"
      ]
    },
    {
      "icon": "shared",
      "extensions": [
        "core"
      ]
    }
  ],
  "vsicons.associations.files": [
    {
      "icon": "maven",
      "extensions": [
        "mvnw"
      ],
      "filename": "true"
    },
    {
      "icon": "commitizen",
      "filenamesGlob": [
        "cz"
      ],
      "extensionsGlob": [
        "toml",
        "yaml"
      ],
      "format": "svg"
    },
    {
      "icon": "sdkman",
      "extensions": [
        ".sdkmanrc"
      ],
      "format": "svg",
      "filename": "true"
    },
    {
      "icon": "golangci",
      "filenamesGlob": [
        "golangci"
      ],
      "extensionsGlob": [
        "yaml",
        "yml",
        "toml",
        "json"
      ],
      "format": "svg"
    },
    {
      "icon": "ruff",
      "filenamesGlob": [
        "ruff"
      ],
      "extensionsGlob": [
        "toml"
      ],
      "format": "svg"
    },
    {
      "icon": "gotmpl",
      "extensions": [
        "tmpl",
        "tpl"
      ],
      "format": "svg"
    },
    {
      "icon": "goreleaser",
      "extensions": [
        ".goreleaser.yaml"
      ],
      "format": "svg",
      "filename": "true"
    },
    {
      "icon": "pytest",
      "extensions": [
        "pytest.ini"
      ],
      "format": "svg",
      "filename": true
    }
  ],
  "vsicons.customIconFolderPath": "/home/rellyson/.config/VSCodium/User"
}
